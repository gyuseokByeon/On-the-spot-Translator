{
  "name": "Translator except Name",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Easily select sentences by mouse over and click, then can see the translated result keeping original named entity.",
  "default_locale": "en",
  
  "permissions" : [
    "http://*/*", 
    "https://*/*", 
    "storage", 
    "clipboardWrite"
  ],
  
  "icons" : {
    "48" : "icon.png"
  },
  
  "content_scripts": [
        {
            "matches": [
                "http://*/*", 
                "https://*/*"
            ],
            "js": [
                "jquery-2.2.0.min.js",
                "menu.js",
                "content.js"
            ],
            "run_at" : "document_start"
        }
    ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
